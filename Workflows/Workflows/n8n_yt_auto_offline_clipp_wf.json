        "parameters": {
          "jsCode": "// Collect all accumulated file URLs from workflow static data\n// This runs AFTER the loop completes\nconst staticData = $getWorkflowStaticData('global');\nconst executionFolder = staticData.executionFolder;\nconst fileUrls = staticData.fileUrls || [];\n\nconsole.log(`[Collect Files] Total files collected: ${fileUrls.length}`);\n\n// Remove duplicates and filter out files already in execution folder\nconst uniqueUrls = [...new Set(fileUrls)].filter(url => {\n  return !url.includes(`/${executionFolder}/`);\n});\n\nconsole.log(`[Collect Files] Unique files to organize: ${uniqueUrls.length}`);\n\nreturn {\n  json: {\n    execution_folder: executionFolder,\n    file_urls: uniqueUrls,\n    total_files: uniqueUrls.length\n  }\n};"
        },
